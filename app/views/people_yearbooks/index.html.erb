<body>
	<div id="wrapper">
		<div id="content">
			<div class="c1">
				<div class="controls">
					<nav class="links">
						<ul>
							<li><a href="/people#index" class="ico1">People </a></li>
							<li><a href="/people_yearbooks#index" class="ico2">People Yearbooks </a></li>
						</ul>
					</nav>
					<div class="profile-box">
						<span class="profile">
							<a href="#" class="section">
								<img class="image" src="/images/img1.png" alt="image description" width="26" height="26" />
								<span class="text-box">
									Welcome
									<strong class="name">Xulin Zhuang</strong>
								</span>
							</a>
							<a href="#" class="opener">opener</a>
						</span>
						 <%= link_to('Logout', destroy_user_session_path, :method => :delete,class: 'btn-on') %>  
					</div>
				</div>
				<div class="tabs">
					<div id="tab-1" class="tab">
						<article>
							<div class="text-section">
								<h1><p id="notice"><%= notice %></p></h1>
							</div>
							<ul class="states" >
								<h1>Listing People Yearbooks</h1>

								<table>
								  <thead>
								    <tr>
									  <th>Person ID</th>
								      <th colspan="3"></th>
								    </tr>
								  </thead>

								  <tbody>
								    <% @people_yearbooks.each do |people_yearbook| %>
								      <tr>
										 <td><%= people_yearbook.person_id %></td>
										 <td><%= people_yearbook.event_description %></td>
								        <td><%= link_to 'Show', people_yearbook %></td>
								        <td><%= link_to 'Edit', edit_people_yearbook_path(people_yearbook) %></td>
								        <td><%= link_to 'Destroy', people_yearbook, method: :delete, data: { confirm: 'Are you sure?' } %></td>
								      </tr>
								    <% end %>
								  </tbody>
								</table>

								<br>

								<%= link_to 'New People yearbook', new_people_yearbook_path %>
								
							</ul>
						</article>
					</div>
				</div>
			</div>
		</div>
		
		
	</div>
</body>
